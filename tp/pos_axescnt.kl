PROGRAM pos_axescnt
%NOBUSYLAMP
%NOLOCKGROUP

VAR
	out_reg : INTEGER
	reg_no : INTEGER
	grp_no : INTEGER
%from tpe.klh %import get_int_arg
%from registers.klh %import set_int
%from posetp.klh %import num_of_axes

BEGIN
	reg_no = tpe__get_int_arg(1)
	grp_no = tpe__get_int_arg(2)
	out_reg = tpe__get_int_arg(3)
	registers__set_int(out_reg, posetp__num_of_axes(reg_no,grp_no))
END pos_axescnt