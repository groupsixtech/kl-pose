PROGRAM pos_setjnt2
%NOBUSYLAMP
%NOLOCKGROUP

VAR
	j1 : REAL
	j2 : REAL
	pr_num : INTEGER
	grp_no : INTEGER
%from tpe.klh %import get_real_arg,get_int_arg
%from posetp.klh %import set_pr_jpos2

BEGIN
	j1 = tpe__get_real_arg(1)
	j2 = tpe__get_real_arg(2)
	pr_num = tpe__get_int_arg(3)
	grp_no = tpe__get_int_arg(4)
	posetp__set_pr_jpos2(j1,j2,pr_num,grp_no)
END pos_setjnt2